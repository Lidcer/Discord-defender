{
  "name": "discord-defender",
  "version": "1.0.0",
  "description": "Software that is monitoring infected files and tries to restore them",
  "main": "index.js",
  "scripts": {
    "pack": "electron-builder --dir",
    "dist": "electron-builder",
    "webpack": "webpack",
    "ts": "tsc",
    "ts-w": "tsc -w",
    "nodemon": "nodemon --inspect dist/index.js",
    "start": "electron --trace-warnings .",
    "node": "node dist/index.js",
    "dev": "concurrently --prefix \"[{name}]\" --names \"TS,DEFENDER\" -c \"bgBlue.bold,bgRed.bold\" \"npm run ts-w\" \"npm run nodemon\""
  },
  "keywords": [
    "lidcer",
    "discord",
    "defender",
    "pirate",
    "stealer",
    "token",
    "grabber",
    "anti",
    "virus",
    "malware"
  ],
  "author": "Lidcer",
  "license": "MIT",
  "devDependencies": {
    "@fortawesome/react-fontawesome": "^0.1.16",
    "@types/auto-launch": "^5.0.2",
    "@types/proper-lockfile": "^4.1.2",
    "@types/react-dom": "^17.0.11",
    "@types/styled-components": "^5.1.18",
    "@typescript-eslint/eslint-plugin": "^5.8.0",
    "@typescript-eslint/parser": "^5.8.0",
    "@webpack-cli/generators": "^2.4.1",
    "electron": "^16.0.5",
    "electron-builder": "^22.14.5",
    "eslint": "^8.5.0",
    "nodemon": "^2.0.15",
    "typescript": "^4.5.4",
    "webpack": "^5.65.0",
    "webpack-cli": "^4.9.1"
  },
  "dependencies": {
    "@babel/plugin-proposal-class-properties": "^7.16.5",
    "@babel/plugin-proposal-decorators": "^7.16.5",
    "@babel/plugin-proposal-numeric-separator": "^7.16.5",
    "@babel/plugin-proposal-object-rest-spread": "^7.16.5",
    "@babel/plugin-transform-runtime": "^7.16.5",
    "@babel/preset-env": "^7.16.5",
    "@babel/preset-react": "^7.14.5",
    "@babel/preset-typescript": "^7.16.5",
    "@electron/remote": "^2.0.1",
    "@fortawesome/fontawesome-svg-core": "^1.2.36",
    "@fortawesome/free-solid-svg-icons": "^5.15.4",
    "auto-launch": "^5.0.5",
    "babel-loader": "^8.2.3",
    "concurrently": "^6.5.1",
    "moment": "^2.29.1",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "styled-components": "^5.3.3",
    "ts-loader": "^9.2.6"
  },
  "build": {
    "productName": "Discord Defender",
    "appId": "discord.defender",
    "directories": {
      "output": "electron/output",
      "buildResources": "dist"
    },
    "files": [
      "**/*",
      "dist/*"
    ],
    "extraFiles": [
      "dist/**/*",
      "index.html",
      "icon.png"
    ]
  }
}
